class Group {
private:
  string name;
  string specialization;
  int studentsCount;
public:
  Group(string name, string specialization, int studentsCount) {
    this->name = name;
    this->specialization = specialization;
    this->studentsCount = studentsCount;
  }

  string getName() {
    return name;
  }

  string getSpecialization() {
    return specialization;
  }

  int getStudentsCount() {
    return studentsCount;
  }

  void setName(string name) {
    this->name = name;
  }

  void setSpecialization(string specialization) {
    this->specialization = specialization;
  }

  void setStudentsCount(int studentsCount) {
    this->studentsCount = studentsCount;
  }
};

class Address {
private:
  string country;
  string region;
  string city;
  string street;
  int houseNumber;
  char korpus;
public:
  Address(string country, string region, string city, string street, int houseNumber, char korpus) {
    this->country = country;
    this->region = region;
    this->city = city;
    this->street = street;
    this->houseNumber = houseNumber;
    this->korpus = korpus;
  }

  string getCountry() {
    return country;
  }

  string getRegion() {
    return region;
  }

  string getCity() {
    return city;
  }

  string getStreet() {
    return street;
  }

  int getHouseNumber() {
    return houseNumber;
  }

  char getKorpus() {
    return korpus;
  }

  void setCountry(string country) {
    this->country = country;
  }

  void setRegion(string region) {
    this->region = region;
}
void setCity(string city) {
this->city = city;
}
void setStreet(string street) {
this->street = street;
}
void setHouseNumber(int house_number) {
this->house_number = house_number;
}
void setBirthDate(Date birth_date) {
this->birth_date = birth_date;
}
void setZodiac(string zodiac) {
this->zodiac = zodiac;
}
void setStartDate(Date start_date) {
this->start_date = start_date;
}
void setKurs(int kurs) {
this->kurs = kurs;
}
void setGroupName(string group_name) {
this->group_name = group_name;
}
void setSpecialization(string specialization) {
this->specialization = specialization;
}
void setDZRates(int* dz_rates, int dz_count) {
this->dz_rates = dz_rates;
this->dz_count = dz_count;
}
void setPracticeRates(int* practice_rates, int practice_count) {
this->practice_rates = practice_rates;
this->practice_count = practice_count;
}
void setExamRates(int* exam_rates, int exam_count) {
this->exam_rates = exam_rates;
this->exam_count = exam_count;
}
void setZachetRates(int* zachet_rates, int zachet_count) {
this->zachet_rates = zachet_rates;
this->zachet_count = zachet_count;
}
